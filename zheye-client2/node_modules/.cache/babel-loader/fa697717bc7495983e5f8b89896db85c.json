t.commit('setSearchList', value)\n  },\n  setWsMsg(context, value) {\n    context.commit('setWsMsg', value)\n  },\n  setWs(context, value) {\n    context.commit('setWs', value)\n  }\n\n}\nexport default new Vuex.Store({\n  state,\n  mutations,\n  getters,\n  actions\n})\n"]}]}